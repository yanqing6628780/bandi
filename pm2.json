{
    "apps": [{
        "name": "authbox",
        "script": "bin/www",
        "log_date_format": "YYYY-MM-DD HH:mm Z",
        "error_file": "./server-err.log",
        "out_file": "./server-out.log",
        "pid_file": "./server.pid",
        "watch": true,
        "instances": 1,
        "exec_mode" : "fork",
        "env": {
            "NODE_ENV": "development"
        }
    }]
}
