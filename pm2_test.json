{
    "name": "bandai",
    "script": "./bin/www",
    "cwd": "./",
    "instances": 1,
    "exec_mode": "fork",
    "watch": [
        "bin",
        "routers",
        "app",
        "config"
    ],
    "ignore_watch": [
        "node_modules",
        "logs",
        "public"
    ],
    "watch_options": {
        "followSymlinks": false
    },
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "env": {
        "NODE_ENV": "test"
    }
}
